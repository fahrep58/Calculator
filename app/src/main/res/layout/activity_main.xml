<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:id="@+id/Ans">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Answer"
        android:id="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:id="@+id/button"
        android:layout_below="@+id/textView"
        android:layout_alignParentStart="true"
        android:layout_marginTop="73dp"
        android:backgroundTint="#3399FF"
        android:background="#3399FF"
        android:onClick="addToList(one)" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2"
        android:id="@+id/button2"
        android:layout_alignTop="@+id/button"
        android:layout_toEndOf="@+id/button"
        android:backgroundTint="#3399FF"
        android:background="#3399FF" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3"
        android:id="@+id/button3"
        android:layout_alignTop="@+id/button2"
        android:layout_toEndOf="@+id/button2"
        android:backgroundTint="#3399FF"
        android:background="#3399FF" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="+"
        android:id="@+id/button4"
        android:layout_alignTop="@+id/button3"
        android:layout_toEndOf="@+id/button3"
        android:background="#FFCC66" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4"
        android:id="@+id/button5"
        android:layout_below="@+id/button"
        android:layout_alignParentStart="true"
        android:backgroundTint="#3399FF"
        android:background="#3399FF" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5"
        android:id="@+id/button6"
        android:layout_below="@+id/button2"
        android:layout_alignStart="@+id/button2"
        android:backgroundTint="#3399FF"
        android:background="#3399FF" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="6"
        android:id="@+id/button7"
        android:layout_below="@+id/button3"
        android:layout_toEndOf="@+id/button6"
        android:backgroundTint="#3399FF"
        android:background="#3399FF" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-"
        android:id="@+id/button8"
        android:layout_below="@+id/button3"
        android:layout_toEndOf="@+id/button7"
        android:background="#FFCC66" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="7"
        android:id="@+id/button9"
        android:layout_below="@+id/button5"
        android:layout_alignParentStart="true"
        android:backgroundTint="#3399FF"
        android:background="#3399FF" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="8"
        android:id="@+id/button10"
        android:layout_alignTop="@+id/button9"
        android:layout_toEndOf="@+id/button9"
        android:backgroundTint="#3399FF"
        android:background="#3399FF" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="9"
        android:id="@+id/button11"
        android:layout_alignTop="@+id/button10"
        android:layout_toEndOf="@+id/button10"
        android:backgroundTint="#3399FF"
        android:background="#3399FF" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="*"
        android:id="@+id/button12"
        android:layout_alignTop="@+id/button11"
        android:layout_toEndOf="@+id/button11"
        android:background="#FFCC66" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button13"
        android:layout_below="@+id/button10"
        android:layout_alignStart="@+id/button10"
        android:layout_marginTop="120dp"
        android:text="Evaluate"
        android:background="#FFCC66"
        android:layout_toStartOf="@+id/button16" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:id="@+id/button14"
        android:layout_below="@+id/button10"
        android:layout_alignStart="@+id/button10"
        android:backgroundTint="#3399FF"
        android:background="#3399FF" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="."
        android:id="@+id/button15"
        android:background="#3399FF"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/button9"
        android:layout_alignBottom="@+id/button14"
        android:layout_toStartOf="@+id/button10" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="/"
        android:id="@+id/button16"
        android:layout_below="@+id/button12"
        android:layout_alignStart="@+id/button12"
        android:background="#FFCC66" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="%"
        android:id="@+id/button17"
        android:layout_below="@+id/button11"
        android:layout_toStartOf="@+id/button16"
        android:background="#FFCC66" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Expression"
        android:id="@+id/textView2"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />
</RelativeLayout>
